/**index.wxss**/

.blue-group {
  background: #1b82d2;
  padding: 0 20rpx;
}

.placeholder-input {
  color: white;
}

.blue-group .top-group {
  width: 100%;
  display: flex;
  justify-content: space-between;
  align-items: center;
  flex: 1;
}

.blue-group .top-group input {
  height: 48rpx;
  flex: 1;
  padding: 0 20rpx 0 50rpx;
  background-color: rgba(255, 255, 255, 0.2);
  border-radius: 10rpx;
  font-size: 24rpx;
  color: white;
  background-image: url('data:image/png;base64,');
  background-size: 36rpx;
  background-position: 10rpx 50%;
  background-repeat: no-repeat;
}

.blue-group  .right-group {
  flex-basis: 120rpx;
  display: flex;
  justify-content: space-evenly;
  padding: 0 0 0 20rpx;
}

.blue-group  .right-group image {
  width: 42rpx;
  height: 42rpx;
}

.main-menu-group {
  display: flex;
  justify-content: space-around;
  margin-top: 30rpx;
}

.main-menu-group .main-menu {
  width: 120rpx;
  display: flex;
  flex-direction: column;
  align-items: center;
  margin-bottom: 30rpx;
}

.main-menu-group .main-menu image {
  width: 64rpx;
  height: 64rpx;
}

.main-menu-group .main-menu text {
  color: white;
  font-size: 28rpx;
  margin: 10rpx 0;
}

.white-group {
  display: flex;
  flex-wrap: wrap;
  justify-content: space-around;
  padding: 20rpx 0;
}

.white-group .white-item {
  width: 160rpx;
  display: flex;
  flex-direction: column;
  align-items: center;
  margin: 20rxp;
}

.white-group .white-item image {
  width: 64rpx;
  height: 64rpx;
}

.white-group .white-item text {
  font-size: 28rpx;
  margin: 20rpx 0;
}

.server-group {
  width: 100%;
  height: 168rpx;
  display: flex;
  align-items: center;
  border-top: 26rpx solid rgba(0, 0, 0, 0.1);
  border-bottom: 26rpx solid rgba(0, 0, 0, 0.1);
  padding: 0 20rpx;
}

.server-group image {
  width: 120rpx;
  height: 120rpx;
}

.server-group .list-group {
  flex: 1;
}

.list-group view {
  color: black;
  font-size: 32rpx;
  margin: 16rpx 0;
}

.list-group view::before {
  content: "▪";
  width: 12rpx;
  height: 12rpx;
}

.list-group view text {
  color: gray;
  font-size: 32rpx;
}

.server-group .right-r {
  flex-basis: 40rpx;
  padding: 0 60rpx;
  position: relative;
}

.server-group .right-r:after {
  content: " ";
  display: inline-block;
  height: 6px;
  width: 6px;
  border-width: 2px 2px 0 0;
  border-color: #c8c8cd;
  border-style: solid;
  -webkit-transform: matrix(0.71, 0.71, -0.71, 0.71, 0, 0);
  transform: matrix(0.71, 0.71, -0.71, 0.71, 0, 0);
  position: relative;
  top: -2px;
  /* position: absolute; */
  top: 50%;
  margin-top: -4px;
  right: 2px;
}

.banner-group {
  width: 100%;
  border-bottom: 26rpx solid rgba(0, 0, 0, 0.1);
}

.banner-group .banner-item {
  width: 100%;
}

.banner-group .banner-item image{
  width: 100%;
}